<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>transferBean</managed-bean-name>
  <managed-bean-class>ch.hevs.managedbeans.TransferBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanOwner</managed-bean-name>
  <managed-bean-class>ch.hevs.managedbeans.BeanOwner</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanHouse</managed-bean-name>
  <managed-bean-class>ch.hevs.managedbeans.BeanHouse</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>beanLocation</managed-bean-name>
  <managed-bean-class>ch.hevs.managedbeans.BeanLocation</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <display-name>/welcomeHouseselling.xhtml</display-name>
  <from-view-id>/welcomeHouseselling.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addChalet</from-outcome>
   <to-view-id>/addChalet.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showHouses</from-outcome>
   <to-view-id>/showHouses.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>transferHouse</from-outcome>
   <to-view-id>/transferHouse.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showOwners</from-outcome>
   <to-view-id>/showOwners.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showLocations</from-outcome>
   <to-view-id>/showLocations.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addVilla</from-outcome>
   <to-view-id>/addVilla.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/showOwners.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>updateOwner</from-outcome>
   <to-view-id>/updateOwner.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/addChalet.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addOwner</from-outcome>
   <to-view-id>/addOwner.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addLocation</from-outcome>
   <to-view-id>/addLocation.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/addVilla.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>addOwner</from-outcome>
   <to-view-id>/addOwner.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addLocation</from-outcome>
   <to-view-id>/addLocation.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
